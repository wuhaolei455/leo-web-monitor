{
  "name": "leo-web-monitor",
  "version": "1.2.3",
  "description": "A lightweight JavaScript runtime error monitoring SDK for capturing and reporting frontend errors",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "examples"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build && npm run test",
    "publish:patch": "node scripts/publish.js patch",
    "publish:minor": "node scripts/publish.js minor",
    "publish:major": "node scripts/publish.js major",
    "publish:dry": "npm pack --dry-run",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "keywords": [
    "error-monitoring",
    "javascript",
    "typescript",
    "sdk",
    "error-tracking",
    "frontend",
    "browser",
    "monitoring",
    "crash-reporting",
    "error-handler"
  ],
  "author": "Leo",
  "license": "MIT",
  "homepage": "https://github.com/whl555/leo-web-monitor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whl555/leo-web-monitor.git"
  },
  "bugs": {
    "url": "https://github.com/whl555/leo-web-monitor/issues"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "rollup": "^3.29.4",
    "rollup-plugin-dts": "^5.3.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  }
}
